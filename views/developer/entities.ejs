<!DOCTYPE html>
<html>
<head>
    <% include ../partials/master/header %>
</head>

<body class="fixed-navigation">

<div id="wrapper">

    <% include partials/leftPanel %>

    <div id="page-wrapper" class="white-bg">

        <% include partials/topPanel %>

        <div class="wrapper wrapper-content">

        <% include ../partials/master/message %>

            <div class="row white-bg">
                <div class="col-lg-12">
                    <h1>Entities and Attributes</h1>
                    The following entities and attributes are available for reporting.
                    <br><br>
                </div>
            </div>

            <% for (var e = 0; e < entities.length; e++) { %>
            <div class="row">
                <div class="col-md-12">
                    <a data-toggle="collapse" href="#faq1"><span class="fa fa-angle-down"></span>&nbsp; <%- entities[e].name %></a>
                </div>
            </div>
            <% } %>

            <div class="row white-bg">
                <div class="col-lg-12">
                    <table class="table">
                        <thead>
                        <tr>
                            <th>Name</th>
                            <th>Title</th>
                            <th>Description</th>
                            <th>Type</th>
                        </tr>
                        </thead>
                        <tbody>
                        <% var attributes = []; for (var i=0; i < attributes.length; i++) { %>
                        <tr>
                            <td><%=attributes[i].name %></td>
                            <td><%=attributes[i].title %></td>
                            <td><%=attributes[i].description %></td>
                            <td><%=attributes[i].dataType %></td>
                        </tr>
                        <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        <% include ../partials/master/footer %>
    </div>
</div>

<% include ../partials/master/footerScripts %>

</body>
</html>
